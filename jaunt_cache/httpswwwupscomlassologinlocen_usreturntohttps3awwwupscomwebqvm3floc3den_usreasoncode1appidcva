<script>

document.getElementById("email").addEventListener("blur", function() {

	userIDOnBlur("userID");
		
}, false);

document.getElementById("pwd").addEventListener("blur", function() {
	
	passwordOnBlur("password");
		
}, false);


	var pwShown = 0;

	document.getElementById("showHide").addEventListener("click", function() {
		if (pwShown == 0) {
			pwShown = 1;
			show_pwd("showHide", "pwd","Hide");
		} else {
			pwShown = 0;
			hide_pwd("showHide", "pwd","Show");
		}
	}, false);

	function login(provider) {
		gigya.socialize.login({
			provider : provider,
			callback : "onLoginHandler"
		});
	}
</script>

<script>
	// This method is activated when the page is loaded

	function onLoad() {

		// register for login event	
		gigya.socialize.addEventHandlers({

			context : {
				str : 'congrats on your'
			}

			,
			onLogin : onLoginHandler

			,
			onLogout : onLogoutHandler

		});
		
		var uidError= '';
		
		if(uidError != null && uidError){
			$("#email").siblings(".ups-icons-error").show();
			$("#email").addClass("ng-invalid ng-dirty");
		}

	}

	// onLogin Event handler

	function onLoginHandler(eventObj) {

		if($("#ups-modal_title").length != 0)
			{
			processConnectWithSocialMedia('X'+ JSON.stringify(eventObj));
			return;
			}
		submitForm('LoginTest', 'connectWithSocial', 'X'
				+ JSON.stringify(eventObj));

	}

	// onLogout Event handler

	function onLogoutHandler(eventObj) {

		document.getElementById('status').style.color = "red";

		document.getElementById('status').innerHTML = "Status: You are now signed out";

	}

	onLoad();
		
	
</script>

